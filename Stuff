package Vertretungsplan;

public class Stuff {

/** Überprüft, wie viele Felder eines Arrays belegt sind
  * @param vertretung der zu überprüfende Array (Datentyp, des Arrays, ist eine Instanz der Klasse Vetretung)
  * @return anzahl der belegten Felder 
  */
  public static int tatsächlicheLänge(Vertretung[] vertretungen)             
    {
        int zähler = 0;
        
        for (Vertretung v : vertretungen)
        {
            if (v!=null)
            {
                zähler++;
            }
        }
        return zähler;
    }
    
/** Überprüft, wie viele Felder eines Arrays belegt sind
  * @param vertretung der zu überprüfende Array (Datentyp, des Arrays, ist ein String)
  * @return anzahl der belegten Felder 
  */
    
    public static int tatsächlicheLänge(String[] start)
    {
        int zähler = 0;
        
        for (String s : start)
        {
            if (s!=null)
            {
                zähler++;
            }
        }
        return zähler;
    }
    
}
